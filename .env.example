# Docker Compose Environment Configuration
# Copy this file to .env and customize the values for your deployment

# Backend Configuration
BACKEND_PORT=8000
BACKEND_LOG_LEVEL=info

# Frontend Configuration
FRONTEND_PORT=3000

# API Configuration
# Note: You need to create the following files in the backend directory:
# - backend/nvdev_api.txt (NVIDIA API key)
# - backend/tavily_api.txt (Tavily API key)
# - backend/openai_api.txt (OpenAI API key, optional)

# LLM Configuration
DEFAULT_MODEL=llama-3.1-nemotron-253b
LLM_BASE_URL=https://integrate.api.nvidia.com/v1

# Research Configuration
MAX_TOPICS=1
MAX_SEARCH_PHRASES=1
MAX_SEARCH_RESULTS=10
